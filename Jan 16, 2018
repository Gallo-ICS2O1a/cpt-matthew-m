def setup():
    size(400, 400)
    background(204, 102, 0)


def draw():
    line(125, 50, 125, 350)
    line(280, 50, 280, 350)
    line(370, 130, 30, 130)
    line(370, 260, 30, 260)


def mousePressed():

    if mouseButton == LEFT:
        b = True
        fill(18, 657, 754)
        ellipse(pmouseX, pmouseY, 40, 40)
    elif mouseButton == RIGHT:
        b = False
        fill(300, 60, 78)
        rect(pmouseX, pmouseY, 40, 40)
